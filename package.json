{
	"name": "leakexpert-automation",
	"version": "1.0.0",
	"description": "leakexpert automation framework",
	"main": "index.js",
	"scripts": {
		"test": "node_modules/.bin/wdio ./src/config/suite.cucumber.conf.js",
		"allure-report": "node_modules/.bin/allure generate --clean ./test/reports/allure-results && allure open",
		"junit-report": "node_modules/.bin/junit-viewer --results=./test/reports/junit-results --save=junit-results.html && open junit-results.html"
	},
	"keywords": [
		"selenium",
		"web",
		"testing",
		"webdriverjs",
		"webdriver",
		"automation"
	],
	"author": "Khiem Luc",
	"license": "ISC",
	"dependencies": {
		"cucumber": "6.0.2",
		"cucumber-tag-expressions": "1.1.1",
		"@babel/cli": "^7.5.0",
		"@babel/core": "^7.5.0",
		"@babel/preset-env": "^7.2.3",
		"@babel/register": "^7.0.0",
		"@wdio/allure-reporter": "^5.11.0",
		"@wdio/cli": "^5.11.0",
		"@wdio/cucumber-framework": "5.16.10",
		"@wdio/dot-reporter": "5.16.5",
		"@wdio/junit-reporter": "5.15.5",
		"@wdio/local-runner": "5.16.10",
		"@wdio/spec-reporter": "5.16.5",
		"@wdio/sync": "5.16.10",
		"allure-commandline": "^2.13.0",
		"assert": "^2.0.0",
		"chai": "^4.1.2",
		"wdio-json-reporter": "^1.0.0",
		"webdriverio": "5.16.10"
	},
	"devDependencies": {
		"@wdio/selenium-standalone-service": "^5.11.2",
		"eslint": "^6.6.0",
		"eslint-config-standard": "^14.1.0",
		"eslint-plugin-import": "^2.7.0",
		"eslint-plugin-node": "^10.0.0",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-react": "^7.16.0",
		"eslint-plugin-standard": "^4.0.1"
	},
	"babel": {
		"presets": [
			[
				"@babel/preset-env",
				{
					"targets": {
						"node": "current"
					}
				}
			]
		]
	}
